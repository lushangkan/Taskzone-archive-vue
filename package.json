{
  "name": "taskzone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets",
    "remove:sql:wasm": "rimraf public/assets/sql-wasm.wasm",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "android:start": "npm run remove:sql:wasm && npm run build:native && npx cap sync && npx cap copy && npx cap open android"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^5.0.5-2",
    "@capacitor/android": "^5.0.5",
    "@capacitor/core": "^5.0.5",
    "@capacitor/ios": "^5.0.5",
    "animate.css": "^4.1.1",
    "animejs": "^3.2.1",
    "colorjs.io": "^0.4.5",
    "jeep-sqlite": "^2.3.7",
    "lucide-vue-next": "^0.244.0",
    "moment": "^2.29.4",
    "pinia": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "tailwindcss-animatecss": "^3.0.4",
    "typeorm": "^0.3.17",
    "vue": "^3.3.4",
    "vue-feather-icons": "^5.1.0",
    "vue-i18n": "9",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/animejs": "^3.1.7",
    "@types/node": "^18.16.17",
    "@types/vue-feather-icons": "^5.0.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@yuo/postcss-px2vw": "^1.0.5",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "4.9.5",
    "unocss": "^0.53.1",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
